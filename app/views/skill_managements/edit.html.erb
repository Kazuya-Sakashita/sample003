<h1>SkillManagements#edit</h1>
<p>Find me in app/views/skill_managements/edit.html.erb</p>

<h2>スキル修正 </h2>
<%= form_with model: @skill_management, url: skill_management_path, method: :patch, id: 'edit_skill_management', class: 'edit_skill_management', local: true do |f| %>
  <div class="field">
  <%= f.label :skill_id %><br />
  <%= f.collection_select :skill_id, Skill.all, :id, :skill %>
  </div>

  <div class="field">
  <%= f.label :skill_level %><br />
  <%= f.select :skill_level, Skill.skill_levels.keys.map {|k| [k, k]}, {class: 'form-control', required: true } %>

  </div>  
   
    <%= f.submit "更新" %>
    <%= link_to "戻る", :back  %>  

<% end %>