<h1>エンジニア情報一覧</h1>
<hr>
<%= link_to "管理者トップ画面", admins_managements_path %><br>
<hr>

<table class="">
  <tbody>
    <tr>
      <td>名前</td>
      <td>メールアドレス</td>
      <td>単価</td>
      <td>単価単位</td>
      <td>スキル/スキルレベル</td>

    </tr>


    <% @users.each do |user| %>
        <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.unit_price %></td>
        <td><%= user.unit %></td>
          <% user.skill_managements.each do |skill| %>
            <td><%= skill.skill.skill %>/
              <%= skill.skill_level %></td>
          <% end %>
        </tr>
    <% end %>
  </tbody>
</table>



<%= link_to "戻る", :back  %>  